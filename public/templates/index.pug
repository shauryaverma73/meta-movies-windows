// header
header.header
  .header__wrap
    .container
      .row
        .col-12
          .header__content
            // header logo
            a.header__logo(href='index.html')
              img(src='img/logo.svg' alt='')
            // end header logo
            // header nav
            ul.header__nav
              // dropdown
              li.header__nav-item
                a#dropdownMenuHome.dropdown-toggle.header__nav-link(href='#' role='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false') Home
                ul.dropdown-menu.header__dropdown-menu(aria-labelledby='dropdownMenuHome')
                  li
                    a(href='index.html') Home slideshow bg
                  li
                    a(href='index2.html') Home static bg
              // end dropdown
              // dropdown
              li.header__nav-item
                a#dropdownMenuCatalog.dropdown-toggle.header__nav-link(href='#' role='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false') Catalog
