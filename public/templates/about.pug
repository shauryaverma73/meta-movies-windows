                  li
                    a(href='about.html') About
                  li
                    a(href='signin.html') Sign In
                  li
                    a(href='signup.html') Sign Up
                  li
                    a(href='404.html') 404 Page
              // end dropdown
            // end header nav
            // header auth
            .header__auth
              button.header__search-btn(type='button')
                i.icon.ion-ios-search
              a.header__sign-in(href='signin.html')
